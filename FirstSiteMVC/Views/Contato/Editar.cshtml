@{
    ViewData["Title"] = "Editar contato";
}
<div class="text-center">
    <h1 class="display-4">Editar contato</h1>
</div>

<form>
    <div class="form-group">
        <label for="name" class="form-label">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">
    </div>
    <div class="form-group">
        <label for="email" class="form-label">Email</label>
        <input type="text" class="form-control" placeholder="Digite o E-mail do contato">
    </div>
    <div class="form-group">
        <label for="celular" class="form-label">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o telefone do contato">
    </div>
    <button type="submit" class="btn btn-outline-primary btn-block">Alterar</button>
    <a class="btn btn-outline-secondary btn-block" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>
