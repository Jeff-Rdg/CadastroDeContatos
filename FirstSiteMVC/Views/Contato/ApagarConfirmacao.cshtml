@{
    ViewData["Title"] = "Confirmação apagar contato";
}
<div class="text-center">
    <h3 class="display-4">Você realmente deseja apagar o contato (inserir caminho)?</h3>
    <a class="btn btn-outline-danger btn-block" role="button" asp-controller="Contato" asp-action="Index">Cancelar</a>
    <a class="btn btn-outline-primary btn-block" role="button"asp-controller="Contato" asp-action="Apagar">Confirmar</a>
</div>


